#include <iostream>
using namespace std;
int main (){
	string n, npm, c, j;
	
	cout<<"Masukkan nama : ";
	getline(cin,n);
	cout<<"Masukkan npm : ";
	getline(cin,npm);
	cout<<"Masukkan kelas : ";
	getline(cin,c);
	cout<<"Masukkan Jurusan :";
	getline(cin,j);
	
	cout<<endl;
	
	cout<<"Nama : " <<n<<endl;
	cout<<"NPM :" <<npm<<endl;
	cout<<"Kelas : "<<c<<endl;
	cout<<"Jurusan : " <<j<<endl;
	
	return 0;
}
